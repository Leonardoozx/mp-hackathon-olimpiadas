{
  "name": "ms-olympic-games",
  "version": "1.0.0",
  "description": "Olympic Games micro service",
  "main": "dist/app/index.js",
  "scripts": {
    "start:http": "node dist/app/index.js",
    "start:dev:http": "nodemon --watch src/ --exec ts-node src/app/index.ts -e ts --inspect",
    "test": "tsc && jest tests --colors",
    "test:debug": "npm test --detectOpenHandles --forceExit",
    "test:watch": "tsc && jest tests --watch ",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.proto src/**/*.gql dist/",
    "build": "npm run clean && tsc && npm run copy-files",
    "lint": "eslint src/ tests/",
    "preinstall": "npx npm-force-resolutions"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.3",
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.17",
    "@types/jest": "^28.1.6",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.182",
    "@types/luxon": "^3.2.0",
    "@types/node": "^18.11.19",
    "@types/redis": "^2.8.32",
    "@types/redis-mock": "^0.17.1",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.57.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.33.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "redis-mock": "^0.56.3",
    "rimraf": "^4.1.2",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@graphql-tools/load-files": "^6.6.0",
    "@graphql-tools/merge": "^8.3.0",
    "@graphql-tools/schema": "^9.0.16",
    "@grpc/grpc-js": "^1.7.3",
    "@grpc/proto-loader": "^0.7.0",
    "axios": "^1.2.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.8.0",
    "helmet": "^6.0.1",
    "joi": "^17.6.0",
    "js-yaml": "^4.1.0",
    "json-refs": "^3.0.15",
    "lodash": "^4.17.21",
    "luxon": "^3.2.1",
    "newrelic": "^9.7.5",
    "redis": "^3.1.2",
    "swagger-ui-express": "^4.5.0",
    "type-graphql": "^1.1.1",
    "winston": "^3.13.1"
  },
  "resolutions": {
    "glob-parent": "6.0.1"
  }
}
